public class Calculadora {
    double num1;
    double num2;


    public double Soma(){
        return(num1 + num2);

    }
    public double Subtracao(){
        return(num1 - num2);
    }
    public double  Divisao(){
        return(num1 / num2);

    }
    public double Multiplicacao(){
        return(num1 * num2);
    }
}
